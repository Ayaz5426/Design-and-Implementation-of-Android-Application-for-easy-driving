<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drivezy">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/my_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/my_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity"></activity>
        <activity
            android:name=".rtouploadmain"
            android:label="@string/title_activity_rtouploadmain"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".owpage11" />
        <activity
            android:name=".ImagesActivity"
            android:label="@string/title_activity_images"
            android:theme="@style/AppTheme.NoActionBar" /> <!-- <activity android:name=".owDocImage" /> -->
        <activity android:name=".rtosearch" />
        <activity android:name=".rtohome" />
        <activity android:name=".owpage1" />
        <activity android:name=".ownerlogin" />
        <activity
            android:name=".rtologin"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>